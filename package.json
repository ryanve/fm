{
  "name": "fm",
  "description": "Function modulation utility module",
  "version": "0.8.0",
  "homepage": "https://github.com/ryanve/fm",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "main": "fm.js",
  "scripts": {
    "pretest": "jshint fm.js && jshint test.js",
    "test": "node test.js"
  },
  "keywords": [
    "util",
    "utility",
    "callback",
    "function",
    "functions",
    "functional",
    "partial",
    "method",
    "scope",
    "flow",
    "bind",
    "ender",
    "browser",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/fm.git"
  },
  "bugs": {
    "url": "https://github.com/ryanve/fm/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "aok": "^1.9.0",
    "jshint": "^2.9.3"
  },
  "jshintConfig": {
    "asi": true,
    "boss": true,
    "browser": true,
    "eqnull": true,
    "expr": true,
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxcomplexity": 10,
    "maxerr": 10,
    "maxlen": 115,
    "maxparams": 4,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "worker": true
  }
}
